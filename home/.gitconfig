[core]
    editor = "emacsclient -nw -a ''"
	excludesfile = ~/.gitignore_global

[alias]
    log = log --oneline --decorate --graph
    st  = status
    co  = checkout
    cp  = cherry-pick
    a   = add
    rmc = rm --cached
    cm  = commit
    cma = commit --amend
    d   = diff

[ghq]
    root = ~/.go/src
[push]
	default = matching
